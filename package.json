  "devDependencies": {
    "@types/node": "^20.11.7",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
